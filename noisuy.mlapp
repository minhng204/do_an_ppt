        function runCompute(app)

            % --------- 1. Lấy dữ liệu ---------------
            x = str2num(app.NhapdulieuxEditField.Value); %#ok<ST2NM>
            y = str2num(app.NhapdulieuyEditField.Value); %#ok<ST2NM>
            x0 = str2double(app.NhapgiatrinoisuyEditField.Value);

            % --------- 2. Kiểm tra dữ liệu ----------
            if isempty(x) || isempty(y)
                uialert(app.UIFigure, 'X hoặc Y đang để trống.', 'Lỗi');
                return;
            end
            if length(x) ~= length(y)
                uialert(app.UIFigure, 'Số phần tử của X và Y phải bằng nhau.', 'Lỗi');
                return;
            end
            if any(diff(x) == 0)
                uialert(app.UIFigure, 'Giá trị X không được trùng nhau.', 'Lỗi');
                return;
            end

            method = app.ChonphuongphapnoisuyDropDown.Value;

            % --------- 3. Bắt đầu xử lý ----------
            cla(app.UIAxes);

            switch method

                % ===================== NEWTON =====================
                case 'Nội suy Newton'
                    y_inter = NewtonInterpolation(app, x, y, x0);
                    polyStr = NewtonPolynomialString(app, x, y);

                    app.KetquanoisuyTextArea.Value = string(y_inter);
                    app.KetquadathucnoisuyTextArea.Value = polyStr;

                    xx = linspace(min(x), max(x), 400);
                    yy = arrayfun(@(t) NewtonInterpolation(app, x, y, t), xx);

                    plot(app.UIAxes, xx, yy, 'LineWidth', 2); hold(app.UIAxes,'on');
                    scatter(app.UIAxes, x, y, 'filled');
                    scatter(app.UIAxes, x0, y_inter, 80, 'r', 'filled');
                    legend(app.UIAxes, {'Newton','Data','P(x0)'}, 'Location','best');
                    hold(app.UIAxes,'off');

                % ===================== LAGRANGE =====================
                case 'Nội suy Lagrange'

                    y_inter = Lagrange(app, x, y, x0);
                    polyStr = LagrangePolynomialString(app, x, y);

                    app.KetquanoisuyTextArea.Value = string(y_inter);
                    app.KetquadathucnoisuyTextArea.Value = polyStr;

                    xx = linspace(min(x), max(x), 400);
                    yy = arrayfun(@(t) Lagrange(app, x, y, t), xx);

                    plot(app.UIAxes, xx, yy, 'LineWidth', 2); hold(app.UIAxes,'on');
                    scatter(app.UIAxes, x, y, 'filled');
                    scatter(app.UIAxes, x0, y_inter, 80, 'r', 'filled');
                    legend(app.UIAxes, {'Lagrange','Data','P(x0)'}, 'Location','best');
                    hold(app.UIAxes,'off');
            end

            grid(app.UIAxes,'on');
            xlabel(app.UIAxes,'X');
            ylabel(app.UIAxes,'Y');
            title(app.UIAxes, ['Phương pháp: ' method]);

            % ===================== END SWITCH =====================

            %##########################################################
            %------------------ HÀM NỘI SUY NEWTON ------------------
            %##########################################################
            function y0 = NewtonInterpolation(~, x, y, x0)
                n = length(x);
                F = zeros(n,n);
                F(:,1) = y(:);

                for j = 2:n
                    for i = 1:(n-j+1)
                        F(i,j) = (F(i+1,j-1) - F(i,j-1)) / (x(i+j-1) - x(i));
                    end
                end

                y0 = F(1,1);
                prod = 1;
                for k = 1:n-1
                    prod = prod * (x0 - x(k));
                    y0 = y0 + F(1,k+1) * prod;
                end
            end

            function polyStr = NewtonPolynomialString(~, x, y)
                n = length(x);
                F = zeros(n,n);
                F(:,1) = y(:);

                for j = 2:n
                    for i = 1:(n-j+1)
                        F(i,j) = (F(i+1,j-1) - F(i,j-1)) / (x(i+j-1) - x(i));
                    end
                end

                polyStr = "P(x) = " + string(F(1,1));

                for k = 1:n-1
                    coef = F(1,k+1);
                    term = "";
                    for t = 1:k
                        term = term + "(x - " + string(x(t)) + ")";
                    end
                    polyStr = polyStr + " + " + string(coef) + term;
                end
            end

            %##########################################################
            %------------------ HÀM LAGRANGE -------------------------
            %##########################################################
            function y0 = Lagrange(~, x, y, x0)
                n = length(x);
                y0 = 0;
                for i = 1:n
                    Li = 1;
                    for j = 1:n
                        if j ~= i
                            Li = Li * (x0 - x(j)) / (x(i) - x(j));
                        end
                    end
                    y0 = y0 + y(i) * Li;
                end
            end

            function polyStr = LagrangePolynomialString(~, x, y)
                n = length(x);
                parts = strings(1,n);

                for i = 1:n
                    LiStr = string(y(i)) + "*";
                    for j = 1:n
                        if j ~= i
                            LiStr = LiStr + "(x - " + string(x(j)) + ")/(" + string(x(i)-x(j)) + ")";
                        end
                    end
                    parts(i) = LiStr;
                end

                polyStr = "P(x) = " + strjoin(parts, " + ");
            end

        end
        
         function runRegression(app)
            % Lấy dữ liệu
            X = str2num(app.NhapdulieuxEditField_2.Value); %#ok<ST2NM>
            Y = str2num(app.NhapdulieuyEditField_2.Value); %#ok<ST2NM>
            x0 = str2double(app.NhapgiatrinoisuyEditField_2.Value);
            method = app.ChonphuongphapnoisuyDropDown_2.Value;
    
            % Kiểm tra dữ liệu
            if isempty(X) || isempty(Y)
                uialert(app.UIFigure,'X hoặc Y đang để trống','Lỗi');
                return;
            end
            if length(X) ~= length(Y)
                uialert(app.UIFigure,'Số phần tử X và Y phải bằng nhau','Lỗi');
                return;
            end
    
            % Xử lý hồi quy
            switch method
                case 'Tuyến tính'
                    X_reg = [ones(length(X),1) X(:)];
                    b = X_reg \ Y(:);
                    y0 = [1 x0]*b;
                    eqStr = sprintf('y = %.4f + %.4f*x', b(1), b(2));
    
                case 'Hàm mũ'
                    Ylog = log(Y(:));
                    X_reg = [ones(length(X),1) X(:)];
                    b = X_reg \ Ylog;
                    a = exp(b(1));
                    y0 = a*exp(b(2)*x0);
                    eqStr = sprintf('y = %.4f * exp(%.4f * x)', a, b(2));
    
                case 'Logarit'
                    X_reg = [ones(length(X),1) log(X(:))];
                    b = X_reg \ Y(:);
                    y0 = b(1) + b(2)*log(x0);
                    eqStr = sprintf('y = %.4f + %.4f*ln(x)', b(1), b(2));
            end
    
            % Hiển thị kết quả
            app.KetquadathucnoisuyTextArea_2.Value = eqStr;
            app.KetquanoisuyTextArea_2.Value = string(y0);
    
            % Vẽ đồ thị
            cla(app.UIAxes_2);
            xx = linspace(min(X), max(X), 400);
            switch method
                case 'Tuyến tính'
                    yy = b(1) + b(2)*xx;
                case 'Hàm mũ'
                    yy = a*exp(b(2)*xx);
                case 'Logarit'
                    yy = b(1) + b(2)*log(xx);
            end
            plot(app.UIAxes_2, xx, yy, 'LineWidth',2); hold(app.UIAxes_2,'on');
            scatter(app.UIAxes_2, X, Y, 'filled');
            scatter(app.UIAxes_2, x0, y0, 80,'r','filled');
            legend(app.UIAxes_2, {'Regression','Data','y(x0)'}, 'Location','best');
            grid(app.UIAxes_2,'on'); xlabel(app.UIAxes_2,'X'); ylabel(app.UIAxes_2,'Y');
            hold(app.UIAxes_2,'off');
        end
    end

    
